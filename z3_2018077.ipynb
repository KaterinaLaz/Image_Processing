    "import numpy as np\n",
    "from PIL import Image\n",
    "\n",
    "img = Image.open('image3.jpg')\n",
    "\n",
    "for i in range(img.size[0]):\n",
    "\tfor j in range(img.size[1]):\n",
    "\t\tngtv = img.getpixel((i,j))\n",
    "\t\timg.putpixel((i,j),255 - ngtv)\n",
    "\n",
    "img.show()\n",
    "img.save('image3_n.png')"
   ]
